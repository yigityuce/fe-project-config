{
	"typescript.tsdk": "node_modules/typescript/lib/",
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.renderWhitespace": "all",
	"editor.detectIndentation": false,
	"editor.formatOnSave": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"[javascript]": {
		"editor.formatOnSave": true
	},
	"[javascriptreact]": {
		"editor.formatOnSave": true
	},
	"[typescript]": {
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.formatOnSave": true
	},
	"[css]": {
		"editor.formatOnSave": true
	},
	"[scss]": {
		"editor.formatOnSave": true
	},
	"[html]": {
		"editor.formatOnSave": true
	},
	"[json]": {
		"editor.formatOnSave": true
	},
	"search.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"compiled": true,
		"dist": true,
		"dll": true,
		"node_modules": true
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"compiled": true,
		"dist": true,
		"dll": true,
		"node_modules": true
	},
	"json.schemas": [
		{
			"fileMatch": [
				"common/config/app-config.json"
			],
			"url": "./.schema/assets/media-server-backup/common/config/app-config.schema.json"
		}
	],
	"eslint.lintTask.options": "--ext .js,.ts .",
	"eslint.probe": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html"
	],
	"eslint.enable": true,
	"tslint.alwaysShowRuleFailuresAsWarnings": false,
	"tslint.configFile": "./tslint.json",
	"tslint.ignoreDefinitionFiles": true
}
