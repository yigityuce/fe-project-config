{
	"compilerOptions": {
	  "target": "es5",
	  "module": "ESNext",
	  "moduleResolution": "node",
	  "emitDecoratorMetadata": true,
	  "experimentalDecorators": true,
	  "allowSyntheticDefaultImports": true,
	  "sourceMap": true,
	  "noEmit": true,
	  "noEmitHelpers": true,
	  "importHelpers": true,
	  "strictNullChecks": false,
	  "allowJs": true,
	  "skipLibCheck": true,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "resolveJsonModule": true,
	  "isolatedModules": true,
	  "jsx": "react",
	  "lib": [
		"dom",
		"dom.iterable",
		"esnext"
	  ],
	  "baseUrl": "./src",
	  "paths": {
		"@angular/*": ["node_modules/@angular/*"]
	  },
	  "typeRoots": [
		"node_modules/@types"
	  ],
	  "types": [
		"node",
	  ]
	},
	"exclude": [
	  "node_modules",
	  "dist",
	  "build"
	],
	"awesomeTypescriptLoaderOptions": {
	  "forkChecker": true,
	  "useWebpackText": true
	},
	"compileOnSave": false,
	"buildOnSave": false,
	"atom": {
	  "rewriteTsconfig": false
	}
  }
  